{
  "name": "jspm-build",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "baseURL": ".",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "browser-sync start --server --port 8080 --no-open --files '**/*.css, **/*.html, **/*.js'",
    "dev": "jspm unbundle",
    "build": "gulp bundle",
    "unbuild": "gulp unbundle",
    "build-sfx": "jspm unbundle && jspm bundle-sfx src/main.js build/main.bundle.js --no-runtime",
    "builder": "jspm bundle src/main build/src/main.bundle --inject && gulp build",
    "builder-sfx": "gulp bundle-sfx"
  },
